{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.9.0"
    }
  },
  "rewrites": [
    {
      "source": "/visit.php",
      "destination": "/api/visit.php"
    },
    {
      "source": "/",
      "destination": "/api/index.php"
    },
    {
      "source": "/pages/(.*)",
      "destination": "/api/index.php"
    },
    {
      "source": "/panel/(.*)",
      "destination": "/api/index.php"
    }
  ]
}